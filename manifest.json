{
  "manifest_version": 2,
  "name": "GitHub File Tree Resizer",
  "description": "A plugin to resize the file tree panel for GitHub pull requests",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "page": "popup.html"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}